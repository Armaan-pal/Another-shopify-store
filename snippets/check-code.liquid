<div class="check-code">
  <input type="number" id="checkpincode" placeholder="Enter Your pincode">
  <button class="btnpin" onClick="zipper()">Check</button>
</div>
<script>
 let zipcodes =[{{ settings.zipcode}}] 
  function zipper(){
  let code = document.getElementById('checkpincode').value

  if(zipcodes.includes(parseInt(code)){
    alert('yes')
  }else{
    alert('other zipcode')
  }
  }
</script>